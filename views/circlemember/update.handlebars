<div class="row">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title"> Circle Members</h3>
        </div>
        <div class="panel-body">
            <form method="POST" role="form">

                <input type="hidden" name="id" value="{{circleMember.id}}">

                <div class="form-group">
                    <label>User Id</label>
                    <input class="form-control" name="userId" value="{{circleMember.userId}}">
                </div>

                <div class="form-group">
                    <label>Circle Id</label>
                    <input class="form-control" name="circleId" value="{{circleMember.circleId}}">
                </div>

                <button type="submit" class="btn btn-default">Submit Button</button>
                <button type="reset" class="btn btn-default">Reset Button</button>
            </form>
        </div>
    </div>
</div>

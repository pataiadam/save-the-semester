<div class="row">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"> Rates</h3>
    </div>
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-bordered table-hover table-striped">
          <thead>
            <tr>
              <th>id</th>
              <th>circleId</th>
              <th>userId</th>
              <th>actions</th>
            </tr>
          </thead>
          <tbody>
            {{#each rates}}
              <tr>
                <td>{{this.id}}</td>
                <td>{{this.circleId}}</td>
                <td>{{this.userId}}</td>
                <td>
                  <a href="/rate/show?id={{this.id}}" class="btn btn-xs btn-primary" role="button">Show</a>
                  <a href="/rate/update?id={{this.id}}" class="btn btn-xs btn-primary" role="button">Update</a>
                  {{#unless this.deletedAt}}
                    <a href="/rate/delete?id={{this.id}}" class="btn btn-xs btn-danger" role="button">Delete</a>
                  {{/unless}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <div class="text-right">
        <a href="/rate/create">Create new rate <i class="fa fa-arrow-circle-right"></i></a>
      </div>
    </div>
  </div>
</div>
